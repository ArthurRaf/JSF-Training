<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>projectAddEditForm</from-outcome>
			<to-view-id>/ProjectAddEditForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>projectAndReportList</from-outcome>
			<to-view-id>/ProjectsAndReportsList.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>ContactAddEditForm</from-outcome>
			<to-view-id>/ContactAddEditForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>OrganisationCreationForm</from-outcome>
			<to-view-id>/OrganisationCreationForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
